<%- if ((current_user && restricted == true) || (restricted == false)) -%>
  <div id="pdf" class="col-sm-12">
    <a href="https://webapps.cspace.berkeley.edu/#TENANT#/imageserver/blobs/<%= csid %>/content" target="new">view full size in new window</a>
    <object data="https://webapps.cspace.berkeley.edu/#TENANT#/imageserver/blobs/<%= csid %>/content"
            style="padding: 3px;" height="800px" width="600px" class="hrefclass">
